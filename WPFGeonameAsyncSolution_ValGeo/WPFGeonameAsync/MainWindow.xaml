<Window x:Class="WPFGeonameAsync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGeonameAsync"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Button x:Name="BtnBrowse" Content="Browse"  Click="BtnBrowse_Click"/>
            <Button x:Name="BtnExtract" Content="Dezipper"  Click="BtnUnzip_Click"/>
        </StackPanel>
        <DockPanel>
            
        

            

            
            <TextBlock DockPanel.Dock="Bottom" x:Name="txtExecutionTime"  Text="TextBlock"/>
            <ListBox DockPanel.Dock="Top" x:Name="txtCountryInfos">
            </ListBox>
        </DockPanel>
        
    </DockPanel>
</Window>
